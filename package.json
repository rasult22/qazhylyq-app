{
  "name": "vite-react-typescript-tailwind-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "generate-assets": "npx capacitor-assets generate --ios && npx capacitor-assets generate --android",
    "l10n-parse": "npx ttag po2json src/l10n/ru-RU.po > src/l10n/ru-RU.po.json && npx ttag po2json src/l10n/tt-KZ.po > src/l10n/tt-KZ.po.json",
    "l10n-update": "pnpm run lang-tt && pnpm run lang-ru",
    "lang-tt": "npx ttag update src/l10n/tt-KZ.po src/",
    "lang-ru": "npx ttag update src/l10n/ru-RU.po src/",
    "build": "tsc && vite build",
    "serve": "vite preview --port 3000",
    "lint": "eslint . --ext .ts,.tsx,.js",
    "test": "vitest"
  },
  "dependencies": {
    "@capacitor/android": "^5.7.3",
    "@capacitor/app": "^5.0.7",
    "@capacitor/core": "^5.7.3",
    "@capacitor/device": "^5.0.7",
    "@capacitor/ios": "^5.7.3",
    "@fontsource/open-sans": "^5.0.28",
    "capacitor-plugin-safe-area": "^2.0.6",
    "framer-motion": "^11.0.14",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-h5-audio-player": "^3.9.1",
    "ttag": "^1.8.7",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.7.3",
    "@heroicons/react": "^2.0.15",
    "@tailwindcss/forms": "^0.5.3",
    "@types/node": "18.13.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-watch": "^8.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "react-router-dom": "6.11.2",
    "tailwindcss": "^3.2.6",
    "ttag-cli": "^1.10.12",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.32.2"
  }
}
